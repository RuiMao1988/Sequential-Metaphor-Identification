        </attribute>
      </optional>
      <optional>
        <attribute name="sortKey">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">supplies the sort key for this term in an index.</a:documentation>
          <data type="token">
            <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})+</param>
          </data>
        </attribute>
      </optional>
      <empty/>
